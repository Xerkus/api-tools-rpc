{
    "name": "laminas-api-tools/api-tools-rpc",
    "description": "Laminas Module for simplifying the creation of RPC services",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "rpc"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-rpc/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-rpc",
        "rss": "https://github.com/laminas-api-tools/api-tools-rpc/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas-api-tools/api-tools-content-negotiation": "~1.0",
        "laminas/laminas-eventmanager": "~2.3",
        "laminas/laminas-http": "~2.3",
        "laminas/laminas-mvc": "~2.3",
        "laminas/laminas-servicemanager": "~2.3",
        "laminas/laminas-stdlib": "~2.3",
        "laminas/laminas-view": "~2.3",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\Rpc\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "replace": {
        "zfcampus/zf-rpc": "self.version"
    }
}
